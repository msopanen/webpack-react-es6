{
    "automock": false,
    "testRegex": "(/src/main/.*|\\.)(spec.js|spec.jsx)$",
    "moduleFileExtensions": [
        "js","jsx"
    ],
    "snapshotSerializers": [
        "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
}
{
  "name": "react-webpack-ts",
  "version": "1.0.0",
  "description": "React Webpack TypeScript Boilerplate",
  "main": "src/main/index.tsx",
  "scripts": {
    "build": "webpack --config config/production.config.js --colors -p",
    "clean": "rimraf dist node_modules coverage",
    "start": "node config/server.js",
    "dev": "webpack-dev-server --config config/development.config.js",
    "lint": "eslint",
    "test": "jest --config config/jest.json",
    "watch-test": "yarn test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msopanen/webpack-react-es6"
  },
  "keywords": [
    "React",
    "Webpack",
    "ES6",
    "ESlint",
    "Jest",
    "HMR",
    "Flux"
  ],
  "author": "msopanen",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb": "next",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "next",
    "eslint-plugin-react": "^6.10.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
